{
  "schemaVersion": 1,
  "id": "ironchest",
  "version": "${version}",

  "name": "Iron Chests",
  "description": "A complete Fabric port of the original Iron Chest mod. It adds a variety of new chests to the game with varying storage sizes and unique features. Each chest can each be upgraded over time as you collect and store more materials, adding a new system of progression to the game! ",
  "authors": [
    "TechnoVision",
    "cpw"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/iron-chests-fabric",
    "sources": "https://github.com/TechnoVisionDev/IronChest"
  },

  "license": "CC0-1.0",
  "icon": "assets/ironchest/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.technovision.ironchest.IronChests"
    ],
    "client": [
      "com.technovision.ironchest.IronChestsClient"
    ]
  },

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x",
    "libgui": ">=3.0.0-beta.1+1.16.2-rc2"
  },
  "suggests": {
    "flamingo": "*"
  }
}
